	-->
	
	<parent>
		<groupId>org.springframework.boot</groupId>
		<artifactId>spring-boot-starter-parent</artifactId>
		<version>2.1.4.RELEASE</version>
		<relativePath /> <!-- lookup parent from repository -->
	</parent>
	
	 
	<groupId>com.github.shyding</groupId>
	<artifactId>spring-boot-disruptor-sample</artifactId>
	<version>1.0.0-SNAPSHOT</version>

	<packaging>pom</packaging>
	<name>Spring Boot Starter Samples</name>
	<description>Spring Boot Starter Samples</description>

	<modules>
		<module>spring-boot-starter-disruptor</module>
		<module>spring-boot-sample-disruptor</module>
	</modules>
	
</project>